<?xml version="1.0"?>
<package >
  <metadata>
    <id>LovettSoftware.XmlDiff</id>
    <version>1.0.1</version>
    <authors>Chris Lovett</authors>
    <description>A library that compares and merges XML files.</description>
    <projectUrl>https://github.com/microsoft/XmlNotepad/</projectUrl>
    <repository type="git" url="https://github.com/microsoft/XmlNotepad/"/>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <copyright>Copyright Â© 2020 Lovett Software.</copyright>
    <icon>icon.png</icon>
    <tags>xml;xmldiff</tags>
    <dependencies>
      <group targetFramework=".NETFramework4" />
      <group targetFramework=".NETFramework4.5.2" />
      <group targetFramework=".NETFramework4.6" />
      <group targetFramework=".NETFramework4.7" />
      <group targetFramework=".NETFramework4.8" />
      <group targetFramework=".NETStandard2.0" />
    </dependencies>
  </metadata>
  <files>
    <!-- .NET Standard 2.0 -->
    <file src=".\bin\Release\netstandard2.0\*.dll" target="lib\netstandard2.0" />
    <file src=".\bin\Release\netstandard2.0\*.xml" target="lib\netstandard2.0" />
    <!-- .NET Framework 4.8 -->
    <file src=".\bin\Release\net48\*.dll" target="lib\net48" />
    <file src=".\bin\Release\net48\*.xml" target="lib\net48" />
    <file src=".\bin\Release\net48\*.exe" target="lib\net48" />
    <file src=".\bin\Release\net48\*.exe.config" target="lib\net48" />
    <!-- .NET Framework 4.7 -->
    <file src=".\bin\Release\net47\*.dll" target="lib\net47" />
    <file src=".\bin\Release\net47\*.xml" target="lib\net47" />
    <file src=".\bin\Release\net47\*.exe" target="lib\net47" />
    <file src=".\bin\Release\net47\*.exe.config" target="lib\net47" />
    <!-- .NET Framework 4.6 -->
    <file src=".\bin\Release\net46\*.dll" target="lib\net46" />
    <file src=".\bin\Release\net46\*.xml" target="lib\net46" />
    <file src=".\bin\Release\net46\*.exe" target="lib\net46" />
    <file src=".\bin\Release\net46\*.exe.config" target="lib\net46" />
    <!-- .NET Framework 4.5.2 -->
    <file src=".\bin\Release\net452\*.dll" target="lib\net452" />
    <file src=".\bin\Release\net452\*.xml" target="lib\net452" />
    <file src=".\bin\Release\net452\*.exe" target="lib\net452" />
    <file src=".\bin\Release\net452\*.exe.config" target="lib\net452" />
    <!-- .NET Framework 4 -->
    <file src=".\bin\Release\net4\*.dll" target="lib\net4" />
    <file src=".\bin\Release\net4\*.xml" target="lib\net4" />
    <file src=".\bin\Release\net4\*.exe" target="lib\net4" />
    <file src=".\bin\Release\net4\*.exe.config" target="lib\net4" />
    <!-- Assets -->
    <file src="icon.png"/>
  </files>
</package>